<div class="container-fluid">
  <div class="row">

    <div class="col-md-12">
      <div class="row">
        <% property.forEach(function(model){ %>
        <div class="col-md-6">

          <div class="panel panel-default">
            <div class="panel-heading">
              <h3 class="panel-title">
                <a href="/property/view/<%= model.id %>"><%= model.name %></a>
              </h3>
            </div>
            <div class="panel-body">

              <div class="row">
                <div class="col-md-4 col-xs-4">
                  <img alt="image" class="img-rounded" height="150" weight="150" src="<%= model.imageurl %>">
                </div>
                <div class="col-md-8 col-xs-8">

                  <ul class="list-group">
                    <li class="list-group-item"><b>Estate :</b> <%= model.estate %></li>
                    <li class="list-group-item"><b>Gross Area :</b> <%= model.garea %> sq feet</li>
                    <li class="list-group-item"><b>Rent :</b> $<%= model.rent %></li>

                  </ul>

                </div>
              </div>
            </div>

          </div>

        </div>
        <% }) %>
      </div>
    </div>

  </div>
</div>

